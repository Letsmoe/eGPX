{
	"compilerOptions": {
		"moduleResolution": "node",
		"target": "es2022",
		"module": "ES2022",
		"rootDir": "./src/",
		"declaration": true,
		"declarationMap": true,
		"outDir": "./dist/",
		"removeComments": true,
		"declarationDir": "./types/",
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true
	},
	"exclude": ["node_modules", "dist", "types", "schemas", "google"]
}
